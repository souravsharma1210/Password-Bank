<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="@string/version_code"
    android:versionName="@string/version_name"
    package="com.developerAndroid.passwordbank">

    <application
        android:allowBackup="true"

        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.developerAndroid.passwordbank.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <provider
            android:authorities="com.developerAndroid.passwordbank"
            android:name="com.developerAndroid.passwordbank.data.PasswordProvider"
            android:exported="false"/>
    </application>

</manifest>